@prefix : <http://example.org/lang-learning#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:AIM rdf:type owl:Class .
:Topic rdf:type owl:Class .
:Keyword rdf:type owl:Class .

:CheckInAIM rdf:type :AIM ;
            :hasTopic :Airport ;
            :hasKeyword :CheckIn , :BoardingPass , :Passport , :Security ;
            :level "beginner"^^xsd:string .

:Airport rdf:type :Topic ;
         rdfs:label "Airport" .

:CheckIn rdf:type :Keyword ;
         rdfs:label "check-in" .

:BoardingPass rdf:type :Keyword ;
              rdfs:label "boarding pass" .

:Passport rdf:type :Keyword ;
          rdfs:label "passport" .

:Security rdf:type :Keyword ;
          rdfs:label "security" .
